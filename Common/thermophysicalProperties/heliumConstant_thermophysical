FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture     //Helium 
{
    specie
    {
        molWeight       4.00026; //  [g/mol]
    }
    thermodynamics
    {
        Cp              5197.5; // [J/(kg*K)]
        Hf              0;
    }
    transport
    {
        mu              2e-5;    //[Pa*s]
        Pr              0.687;  
    }
}


