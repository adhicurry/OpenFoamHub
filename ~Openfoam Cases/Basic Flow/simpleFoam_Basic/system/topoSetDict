FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // volHeatgen
    {
        name    volHeatgenCellSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (-0.05 -0.05 -0.05) (0.05 0.05 0.05);
    }
    {
        name    volHeatgen;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     volHeatgenCellSet;
    }
);


