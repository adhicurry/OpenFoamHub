FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (4.5 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    fluid_0_to_solid_1
    {
        type            noSlip;
    }
	
    fluid_0_to_solid_2
    {
        type            noSlip;
    }

    frontAndBackPlanes
    {
        type            empty;
    }	

}


