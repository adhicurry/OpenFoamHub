FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    inlet
    {
        type            calculated;
		value		$internalField;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    fluid_0_to_solid_1
    {
        type            zeroGradient;
    }
	
    fluid_0_to_solid_2
    {
        type            zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

}


