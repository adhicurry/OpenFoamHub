FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluid_0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1000;

boundaryField
{

    inlet_air
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.00084;      
        value           uniform 27.07;
        phi             phi;
        k               k;    
    }

    inlet_fuel
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.00011;      
        value           uniform 0.087;
        phi             phi;
        k               k;    
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    fluid_0_to_solid_1
    {
        type            epsilonWallFunction;
		value			$internalField;
    }
	
    fluid_0_to_solid_2
    {
        type            epsilonWallFunction;
		value			$internalField;
    }

    wall-fluid_0
    {
        type            epsilonWallFunction;
		value			$internalField;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

    "proc.*"
    {
        type            processor;
    }
}


