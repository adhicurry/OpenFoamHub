FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluid_0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.015;

boundaryField
{
	
    inlet_air
    {
        type            fixedValue;
        value           uniform 0.950;
    }

    inlet_fuel
    {
        type            fixedValue;
        value           uniform 0.054;
    }
	
    outlet
    {
        type            zeroGradient;
    }

    fluid_0_to_solid_1
    {
        type            kqRWallFunction;
        value           $internalField; 
	}
	
    fluid_0_to_solid_2
    {
        type            kqRWallFunction;
        value           $internalField;  
	}
	
    wall-fluid_0
    {
        type            kqRWallFunction;
        value           $internalField;  
	}
	
    frontAndBackPlanes
    {
        type            empty;
    }
	
    "proc.*"
    {
        type            processor;
    }	
}