FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet_air
    {
        type            surfaceNormalFixedValue;
        refValue        uniform -8;           
    }

    inlet_fuel
    {
        type            surfaceNormalFixedValue;
        refValue        uniform -1.9;          
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (-5 0 0);
    }

    fluid_0_to_solid_1
    {
        type            noSlip;
    }

    fluid_0_to_solid_2
    {
        type            noSlip;
    }

    wall-fluid_0
    {
        type            noSlip;
    }

    "proc.*"
    {
        type            processor;
    }
    
    frontAndBackPlanes
    {
        type            empty;
    }
}


