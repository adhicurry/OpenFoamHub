FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{

    inlet_air
    {
        type            calculated;
        value           $internalField;
    }

    inlet_fuel
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    fluid_0_to_solid_1
    {
        type            calculated;
        value           $internalField;
    }

    fluid_0_to_solid_2
    {
        type            calculated;
        value           $internalField;
    }

    wall-fluid_0
    {
        type            calculated;
        value           $internalField;
    }

    "proc.*"
    {
        type            processor;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}


