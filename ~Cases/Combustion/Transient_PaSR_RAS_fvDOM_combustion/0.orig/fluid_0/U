FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 1; 

boundaryField
{
    "proc.*"
    {
        type            processor;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (-5 0 0);
    }
    frontAndBackPlanes
    {
        type            empty;
    }
    fluid_0_to_solid_1
    {
        type            noSlip;
    }
    fluid_0_to_solid_2
    {
        type            noSlip;
    }
}