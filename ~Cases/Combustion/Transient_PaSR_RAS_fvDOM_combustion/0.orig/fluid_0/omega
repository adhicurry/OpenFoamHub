FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluid_0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 5; 

boundaryField
{
    "proc.*"
    {
        type            processor;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 5;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
	}
	
    frontAndBackPlanes
    {
        type            empty;
    }
    
	"fluid_0_to.*"
    {
        type            omegaWallFunction;
        blending        binomial;
        n               2;
        value           $internalField;
	}
}