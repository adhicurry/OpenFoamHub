FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 1 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    inlet_air
    {
	    type            pressureInletOutletVelocity;
	    value	    uniform (0 1 0);              
    }

    inlet_fuel
    {
	    type            surfaceNormalFixedValue;;
            refValue        uniform -1;             
    }

    outlet
    {
	    type            zeroGradient;
	    value	    uniform (0 1 0);     
    }

    ".*"
    {
        type            noSlip;
    }

    "proc.*"
    {
        type            processor;
    }
    
    frontAndBackPlanes
    {
        type            empty;
    }
}


// ************************************************************************* //
