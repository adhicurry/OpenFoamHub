FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "inlet.*"
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "fluid_0.*"
    {
        type            zeroGradient;
    }

    "wall.*"
    {
        type            zeroGradient;
    }
    
    "proc.*"
    {
        type            processor;
    }
    


    frontAndBackPlanes
    {
        type            empty;
    }
}


