FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 1 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    inlet
    {
	    type            zeroGradient;             
    }

    outlet
    {
	    type            zeroGradient;   
    }

    fluid_0_to_solid_1
    {
        type            noSlip;
    }

    fluid_0_to_solid_2
    {
        type            noSlip;
    }
 
    walls_chimney
    {
        type            noSlip;
    }

    "proc.*"
    {
        type            processor;
    }
    
    frontAndBackPlanes
    {
        type            empty;
    }
}


