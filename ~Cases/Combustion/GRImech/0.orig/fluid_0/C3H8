dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.013;

boundaryField
{
    "proc.*"
    {
        type            processor;
    }
    "proc.*"
    {
        type            processor;
    }
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           uniform 0.013;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           $internalField;

    }
	
    fluid_0_to_solid_1
    {
        type            zeroGradient;
    }
	
	fluid_0_to_solid_2
    {
        type            zeroGradient;
    }

    frontAndBackPlanes
