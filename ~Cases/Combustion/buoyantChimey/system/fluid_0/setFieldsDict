FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
);

regions
(
    boxToCell
    {
        box (0.25 0.5 -0.1) (0.75 1.0 0.2);

        fieldValues
        (
            volScalarFieldValue T 500
        );
    }
);

