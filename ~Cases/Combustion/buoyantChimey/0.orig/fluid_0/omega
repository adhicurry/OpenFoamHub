FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluid_0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet_air
    {
        type            fixedValue;
        value           uniform 5;
    }
	
    inlet_fuel
    {
        type            fixedValue;
        value           uniform 5;
    }
	
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 5;
        value           $internalField; 
	}
	
    fluid_0_to_solid_1
    {
        type            omegaWallFunction;
        value           $internalField; 
	}
	
    fluid_0_to_solid_2
    {
        type            omegaWallFunction;
        value           $internalField; 
	}
	
    walls_fuelinlet
    {
        type            omegaWallFunction;
        value           $internalField; 
	}
	
    frontAndBackPlanes
    {
        type            empty;
    }
	
    "proc.*"
    {
        type            processor;
    }
    
    "symmetry.*" 
    {
        type            symmetry;
    }	
}
