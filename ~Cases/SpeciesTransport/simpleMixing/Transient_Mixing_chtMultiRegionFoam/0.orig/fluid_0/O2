FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid_0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.21;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    inlet_air
    {
        type            fixedValue;
        value           $internalField;
    }
    
    inlet_top
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    inlet_bottom
    {
        type            zeroGradient;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
	
    "wall.*"
    {
        type            zeroGradient;
    }

    symmetry
    {
	type            symmetry;  
    }

    frontAndBackPlanes
    {
        type            empty;
    }

    "proc.*"
    {
        type            processor;
    }

}


