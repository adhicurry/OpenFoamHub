##Reads from the log.solver file and plots values vs iterations
## CHANGE THE FILE PATH IN EACH LINE

set border lw 4
set title "Average Temperature" font "arial,22"
set ylabel "Temperature" font "arial,18"
set xlabel "Iteration" font "arial,18"
set key width 1
plot"< cat /home/adhi-linux/Openfoam2206/SpeciesTRANSPORT/SimpleMixing_Setup/SS_Mixing/log.chtMultiRegionSimpleFoam | grep 'volAverage(fluid_0) of T' | awk '{print $NF}'" title 'Air' with lines lw 4,\
	"< cat /home/adhi-linux/Openfoam2206/SpeciesTRANSPORT/SimpleMixing_Setup/SS_Mixing/log.chtMultiRegionSimpleFoam | grep 'volAverage(solid_1) of T' | awk '{print $NF}'" title 'solid_1' with lines lw 4,\
	"< cat /home/adhi-linux/Openfoam2206/SpeciesTRANSPORT/SimpleMixing_Setup/SS_Mixing/log.chtMultiRegionSimpleFoam | grep 'volAverage(solid_2) of T' | awk '{print $NF}'" title 'solid_2' with lines lw 4,\
	"< cat /home/adhi-linux/Openfoam2206/SpeciesTRANSPORT/SimpleMixing_Setup/SS_Mixing/log.chtMultiRegionSimpleFoam | grep 'average(inlet) of T' | awk '{print $NF}'" title 'inlet' with lines lw 4,\
    "< cat /home/adhi-linux/Openfoam2206/SpeciesTRANSPORT/SimpleMixing_Setup/SS_Mixing/log.chtMultiRegionSimpleFoam | grep 'average(outlet) of T' | awk '{print $NF}'" title 'outlet' with lines lw 4
pause 1
reread


