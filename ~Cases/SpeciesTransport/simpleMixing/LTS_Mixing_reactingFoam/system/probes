// -*- C++ -*-

probesFluid
{
    type            probes;
    libs            (sampling);
    writeControl    timeStep;
    writeInterval   10;
    interpolationScheme cell;
    region          region0;
    log             true;
    verbose         true;

    fields          (p p_rgh U T CH4);

    probeLocations
    (
        (0 0 0)
	(0 0.001 0.001)
    );
}

MassFlowRate_air
{
    type            surfaceFieldValue;
    libs            (fieldFunctionObjects);
    enabled         yes;
    writeControl    timeStep;
    writeInterval   10;
    log             yes;
    writeFields     no;
    regionType      patch;
    name            inlet_air;
    operation       sum;
    fields          (phi);
    region          region0;
}

