FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    inlet_air
    {
        type            fixedValue;
        value           uniform 300;
    }
    inlet_top
    {
        type            fixedValue;
        value           uniform 300;
    }
    
    inlet_bottom
    {
	type		zeroGradient;
    }
    
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           $internalField;
    }

    "wall.*"
    {
	type		zeroGradient;
    }	

    symmetry
    {
	type            symmetry;  
    }

    "proc.*"
    {
        type            processor;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
	
}


