FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 5.7;

    boundaryField
    {
        "inlet"
        {
        	type            fixedValue;
			value		$internalField;
        }	
		
		"outlet"
		{
        	type            inletOutlet;
        	inletValue      $internalField;
        	value           $internalField;
		}
		
		"fluid_0-solid"
		{
        	type            omegaWallFunction;
			value			$internalField;
		}

        "proc.*"
        {
            type            processor;
        }	
		
    }


// ************************************************************************* //
