FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

    boundaryField
    {
        "proc.*"
        {
        type            processor;
        }
        "inlet"
        {
        type            fixedValue;
        value           uniform 300;
        }

		"outlet"
        {
		type            inletOutlet;
        inletValue      uniform 300;
        value           $internalField;
        }

        "fluid_0-solid"
        {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        Tnbr            T;
        kappaMethod     fluidThermo;
        value           uniform 300;
        }
    }



