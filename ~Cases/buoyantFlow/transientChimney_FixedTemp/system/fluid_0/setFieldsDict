FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
);

regions
(
    boxToCell
    {
        box (-0.1275 0.11 0) (-0.11 1 0);

        fieldValues
        (
            volScalarFieldValue T 500
        );
    }
);

