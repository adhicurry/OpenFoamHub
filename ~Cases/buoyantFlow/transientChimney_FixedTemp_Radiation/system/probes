	writeControl    	timeStep;
	writeInterval   	250;
	log				yes;
	writeFields		no;
	operation		average;
	regionType		patch;
	fields			(U T CH4 CO2 H2O O2 p_rgh p);
	name			outlet;
	region			fluid_0;
}


volAveTemp_AIR
{
	type            volFieldValue;
	libs            ("libfieldFunctionObjects.so");
	log             true;
	writeControl    timeStep;//writeTime;//timeStep;//writeTime;
	writeInterval	  250;
	writeFields     false;
	regionType      all;//cellZone;
	region          fluid_0;
	name            fluid_0;
	operation       volAverage;
	weightField     none;//alpha1;
	fields
	(
		T
	);
}


