// -*- C++ -*-

probesFluid
{
    type            probes;
    libs            (sampling);
    writeControl    timeStep;
    writeInterval   50;
    interpolationScheme cell;
    region          fluid_0;
    log             true;
    verbose         true;

    fields          (p p_rgh U T);

    probeLocations
    (
    (0 0 0)			//1
    (0 0.1 0)			//2
    (0 0.25 0)			//3
    (0 0.5 0)			//4			
    );
}

MassFlowRate_Air
{
    type            surfaceFieldValue;
    libs            (fieldFunctionObjects);
    enabled         yes;
    writeControl    timeStep;
    writeInterval   250;
    log             yes;
    writeFields     no;
    regionType      patch;
    name            inlet;
    operation       sum;
    fields          (phi);
    region          fluid_0;
}

