inletAve_Values
{
    // Mandatory entries
	type            	surfaceFieldValue;
    libs            	(fieldFunctionObjects);
	enabled			yes;
	writeControl    	timeStep;
  	writeInterval   	250;
	log			yes;
	writeFields		no;
	operation		average;
	regionType		patch;
	fields			(U T CH4 CO2 H2O O2 p p_rgh);
	name			inlet;
	region			fluid_0;
}

outletAve_values
{
    // Mandatory entries
	type            	surfaceFieldValue;
    libs            	(fieldFunctionObjects);
