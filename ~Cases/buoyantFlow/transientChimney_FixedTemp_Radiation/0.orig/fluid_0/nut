FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0002;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            calculated;
		value			$internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }


    fluid_0_to_solid_1
    {
        type            nutkWallFunction;
		value			$internalField;
    }
	
    fluid_0_to_solid_2
    {
        type            nutkWallFunction;
		value			$internalField;
    }

    walls_chimney
    {
        type            nutkWallFunction;
		value			$internalField;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

    "proc.*"
    {
        type            processor;
    }
    

}


